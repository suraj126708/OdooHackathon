{
  "name": "odoo-hackathon",
  "version": "1.0.0",
  "description": "StackIt Q&A Platform for Odoo Hackathon",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "setup-admin": "cd server && npm run setup-admin",
    "seed": "cd server && npm run seed"
  },
  "keywords": [
    "odoo",
    "hackathon",
    "q&a",
    "stackit"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
